# NPM Configuration for Revenue Service
# ======================================

# Registry settings
registry=https://registry.npmjs.org/

# Package lock file
package-lock=true

# Save exact version
save-exact=true

# Audit settings
audit=false
fund=false

# Scripts
ignore-scripts=false

# Cache settings
cache=~/.npm
cache-min=3600

# Log level
loglevel=warn

# Progress bar
progress=false

# Color output
color=true

# Package.json settings
save-prefix="" 